<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vetclinic.MainController" spacing="10" alignment="CENTER">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Label text="Welcome to the VetSystem" />
    <Button text="View Owners" onAction="#viewOwners" />
    <Button text="View Pets" onAction="#viewPets" />
    <Button text="View Vets" onAction="#viewVets" />
    <Button text="View Appointments" onAction="#viewAppointments" />
</VBox>
